    "deploy:mainnet": "hardhat deploy --network mainnet --tags production --export-all deployments/networks.json",
    "start:dev": "hardhat node --network hardhat  --export-all deployments/networks.json",
    "test": "hardhat test",
    "prepublishOnly": "yarn compile && yarn build && cp contracts/* ."
  },
