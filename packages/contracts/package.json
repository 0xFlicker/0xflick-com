    "build": "tsc --build",
    "compile": "hardhat compile",
    "deploy:localhost": "hardhat deploy --network localhost --export-all deployments/networks.json",
    "mint:localhost": "hardhat mint --network localhost",
    "mint:maticmum": "hardhat mint --network maticmum",
    "mint:matic": "hardhat mint --network matic",
