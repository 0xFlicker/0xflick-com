  "scripts": {
    "build": "yarn image:build && yarn ipfs:origin:build && yarn axolotl:origin:build",
    "image:accept:build": "esbuild src/lambda/image/accept.ts --platform=node --target=node14.19 --bundle --outfile=../../deploy/.layers/image/accept/index.cjs",
    "image:origin:build": "esbuild src/lambda/image/origin.ts --platform=node --target=node14.19 --outfile=../../deploy/.layers/image/origin/index.mjs",
    "image:build": "yarn image:origin:build && yarn image:accept:build && cd .. && yarn build:layer:sharp",
    "ipfs:origin:build": "esbuild src/lambda/ipfs/origin.ts --platform=node --target=node14.19 --outfile=../../deploy/.layers/ipfs/origin/index.mjs",
    "axolotl:origin:build": "esbuild src/lambda/axolotl/index.ts --platform=node --target=node14.19 --bundle --external:canvas --outfile=../../deploy/.layers/axolotl/index.mjs",
    "graphql:build": "esbuild src/lambda/graphql.ts --platform=node --target=node14.19 --bundle --outfile=../../deploy/.layers/graphql/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
